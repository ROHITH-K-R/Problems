int exactly3Divisors(int n)
{  int ans=0;
   bool prime[n+1]; 
    memset(prime, true, sizeof(prime)); 
    prime[0] = prime[1] = 0; 
  
    for (int p=2; p*p<=n; p++) 
    { 
        if (prime[p] == true) 
        { 
           for (int i=p*2; i<=n; i += p) 
              prime[i] = false; 
        } 
    } 
    
    
    for (int i=0;  i*i <= n ; i++) 
        {if (prime[i]) { ans++;} }
          return ans; 
}
