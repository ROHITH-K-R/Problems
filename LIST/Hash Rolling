
//Issue is segmentation fault


#include <iostream>
#include<string>
using namespace std;

int main()
{
   string s="abdbaacaba",p="aba";
   
   int ash,thash,i;
   bool flag=false;
   
   if(p.size()>s.size()){ cout<<"-1"; return 0;}
   
   for(i=0;p.size();i++){
       ash+=int(p[i]-'a')+1;
       thash+=int(s[i]-'a')+1;  }
       
   if(thash==ash){ cout<<s.substr(0,p.size())<<endl;
                     flag=true;    }
   
   for(i=1;i+p.size()-1<s.size();i++){
       
       thash=thash+int(p[i+p.size()-1]-'a')+1-int(p[i-1]-'a')-1;
       
       if(thash==ash){ cout<<s.substr(i,p.size())<<endl;
                         flag=true;    }
                                            }
                                            
    
   if(flag==false){cout<<"-1";} 
   
   return 0;
}

